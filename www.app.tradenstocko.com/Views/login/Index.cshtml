        function getdeviceip() {
            $.ajax({
                url: "https://api.ipify.org",
                type: "get",
                success: function (resp) {
                    localStorage.setItem("devip", resp);
                    var url = window.location.href;
                    $.ajax({
                        url: "/api/userurl?url=" + url,
                        type: "get",
                        success: function (resp) {

                        }
                    });
                }
            }); 
           

        }


        function logincheck() {
            getdeviceip();
            localStorage.setItem("isedit", "false");
            $("#btnsubmit").html(
                `<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> Loading...`
            );
            localStorage.setItem("isdesktopcreen", true);
            var servercode = $("#servercode").val();
            var username = $("#username").val();
            var password = $("#passwd").val();
            var masterrefid = localStorage.getItem("masteruserid");
            var deviceid = getdeviceid();
            localStorage.setItem("pdeviceid", deviceid);
            $.ajax({
                url: "/api/checklogin/?username=" + username + "&password=" + password + "&deviceid=" + deviceid + "&refidbyuser=" + servercode + "&refidformatch=" + masterrefid,
                type: "get",
                success: function (response)
                {
                    if (response == "false") {
                        swal("Please Try Again", "Invalid Login Details");
                        $("#btnsubmit").html(
                            `Submit`
                        );
                    }
                    else if (response == "Bloked") {
                        swal("Please Try Again", "Sorry Your Account Is Blocked");
                        $("#btnsubmit").html(
                            `Submit`
                        );
                    }
