                var isdesktop = checkscreensize();
                if (isdesktop == "true")
                {
                    var ismcx = localStorage.getItem("IsMCXTrade");
                    var isnse = localStorage.getItem("IsNSETrade");
                    var iscds = localStorage.getItem("IsCDSTrade");

                    if (ismcx == "true") {
                        location.href = "/home/mcxfuture";
                    } else if (isnse == "true") {
                        location.href = "/home/nsefuture";
