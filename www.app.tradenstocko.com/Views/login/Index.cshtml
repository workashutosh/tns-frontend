    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
            background: #000000;
            min-height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
            position: relative;
            overflow-x: hidden;
            margin: 0;
            padding: 0;
        }

        .main-login {
            width: 100%;
            position: relative;
            z-index: 1;
        }

        .h-vh {
            min-height: 100vh;
        }

        .login-card {
            background: #1a1a1a;
            border-radius: 16px;
            padding: 32px;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);
            border: 1px solid #333333;
            max-width: 400px;
            width: 100%;
            margin: 0 auto;
        }

        @@keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(40px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .logo-container {
            text-align: center;
            margin-bottom: 32px;
        }

        #t_applogo {
            width: 60px;
            height: 60px;
            margin-bottom: 16px;
        }

        #t_apptitle {
            color: #ffffff;
            font-size: 28px;
            font-weight: 700;
            margin: 0 0 8px 0;
            text-align: center;
            line-height: 1.2;
        }

        .subtitle {
            color: #888888;
            font-size: 16px;
            text-align: center;
            margin-bottom: 32px;
            line-height: 1.4;
        }

        .form-group {
            margin-bottom: 20px;
            position: relative;
        }

        .floating-label-group {
            position: relative;
        }

        .custom-input {
            width: 100%;
            padding: 16px 20px;
            background: #2a2a2a;
            border: 1px solid #404040;
            border-radius: 12px;
            color: #ffffff !important;
            font-size: 16px;
            font-weight: 400;
            transition: all 0.2s ease;
            outline: none;
            box-sizing: border-box;
        }

        .custom-input::placeholder {
            color: #888888;
            font-weight: 400;
        }

        .custom-input:focus {
            background: #2a2a2a;
            border-color: #22c55e;
            box-shadow: 0 0 0 3px rgba(34, 197, 94, 0.1);
        }

        .floating-label {
            position: absolute;
            left: 20px;
            top: 16px;
            color: rgba(255, 255, 255, 0.5) !important;
            font-size: 16px;
            pointer-events: none;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            font-weight: 400;
        }

        .custom-input ~ .floating-label {
            top: 16px;
            left: 20px;
            font-size: 16px;
            color: rgba(255, 255, 255, 0.5) !important;
            background: transparent;
            padding: 0;
        }

        .char-counter {
            position: absolute;
            right: 20px;
            top: 50%;
            transform: translateY(-50%);
            color: rgba(255, 255, 255, 0.4) !important;
            font-size: 12px;
            pointer-events: none;
            font-weight: 500;
        }

        .toggle-password {
            position: absolute;
            right: 20px;
            top: 50%;
            transform: translateY(-50%);
            cursor: pointer;
            padding: 8px;
            z-index: 10;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            color: rgba(255, 255, 255, 0.6);
        }

        .toggle-password:hover {
            transform: translateY(-50%) scale(1.1);
            color: #22c55e;
        }

        .toggle-password svg {
            display: block;
            width: 18px;
            height: 18px;
        }

        .btn {
            width: 100%;
            padding: 16px 20px;
            background: #22c55e;
            border: none;
            border-radius: 12px;
            color: #ffffff !important;
            font-size: 16px;
            font-weight: 600;
            cursor: pointer;
            transition: all 0.2s ease;
            text-transform: none;
            letter-spacing: 0;
            text-decoration: none;
            display: block;
            text-align: center;
            margin-bottom: 24px;
        }

        .btn:hover {
            background: #16a34a;
        }

        .btn:active {
            transform: scale(0.98);
        }

        .login-btn {
            background: linear-gradient(135deg, #22c55e 0%, #16a34a 100%) !important;
        }

        .text-center {
            text-align: center;
        }

        .text-center p {
            color: #888888;
            font-size: 14px;
            margin: 0;
            line-height: 1.5;
        }

        .text-center a {
            color: #22c55e;
            font-weight: 500;
            text-decoration: none;
            transition: all 0.2s ease;
        }

        .text-center a:hover {
            color: #16a34a;
            text-decoration: underline;
        }

        .text-center strong {
            color: #ffffff;
            font-weight: 500;
        }

        form i {
            margin-left: -30px;
            cursor: pointer;
        }

        .disp1 {
            display: none;
        }

        .disp2 {
            display: block;
        }

        .mb-4 {
            margin-bottom: 20px;
        }

        .spinner-border {
            display: inline-block;
            width: 1rem;
            height: 1rem;
            vertical-align: text-bottom;
            border: 0.2em solid currentColor;
            border-right-color: transparent;
            border-radius: 50%;
            animation: spinner-border 0.75s linear infinite;
        }

        @@keyframes spinner-border {
            to { transform: rotate(360deg); }
        }

        .spinner-border-sm {
            width: 0.875rem;
            height: 0.875rem;
            border-width: 0.15em;
        }

        .forgot-password {
            text-align: right;
            margin-top: 8px;
            margin-bottom: 24px;
        }

        .forgot-password a {
            color: #22c55e;
            font-size: 14px;
            text-decoration: none;
            font-weight: 500;
        }

        .forgot-password a:hover {
            color: #16a34a;
            text-decoration: underline;
        }

        .divider {
            display: flex;
            align-items: center;
            margin: 24px 0;
            color: #666666;
            font-size: 14px;
        }

        .divider::before,
        .divider::after {
            content: '';
            flex: 1;
            height: 1px;
            background: #404040;
        }

        .divider::before {
            margin-right: 16px;
        }

        .divider::after {
            margin-left: 16px;
        }

        .social-buttons {
            display: flex;
            gap: 12px;
            margin-bottom: 24px;
        }

        .social-btn {
            flex: 1;
            padding: 12px 16px;
            background: #2a2a2a;
            border: 1px solid #404040;
            border-radius: 12px;
            color: #666666;
            font-size: 14px;
            font-weight: 500;
            text-decoration: none;
            text-align: center;
            transition: all 0.2s ease;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
            cursor: not-allowed;
            opacity: 0.6;
            pointer-events: none;
        }

        .social-btn:hover {
            background: #2a2a2a;
            border-color: #404040;
            color: #666666;
        }

        @@media (min-width: 769px) {
            .disp1 {
                display: block;
            }

            .disp2 {
                display: none;
            }
        }

        @@media (max-width: 768px) {
            .login-card {
                padding: 24px 20px;
                margin: 16px;
                border-radius: 16px;
                max-width: 380px;
            }

            #t_apptitle {
                font-size: 24px;
            }

            #t_applogo {
                width: 50px;
                height: 50px;
            }

            .subtitle {
                font-size: 14px;
            }

            .custom-input {
                padding: 14px 16px;
                font-size: 16px;
            }

            .btn {
                padding: 14px 18px;
                font-size: 15px;
            }

            .social-buttons {
                flex-direction: column;
                gap: 8px;
            }

            .social-btn {
                padding: 14px 16px;
                font-size: 14px;
            }
        }

        @@media (max-width: 480px) {
            .login-card {
                padding: 20px 16px;
                margin: 12px;
                max-width: 340px;
            }

            #t_apptitle {
                font-size: 22px;
            }

            #t_applogo {
                width: 45px;
                height: 45px;
            }

            .subtitle {
                font-size: 13px;
            }

            .custom-input {
                padding: 12px 14px;
                font-size: 15px;
            }

            .btn {
                padding: 12px 16px;
                font-size: 14px;
            }

            .social-btn {
                padding: 12px 14px;
                font-size: 13px;
            }
        }
    </style>
</head>
